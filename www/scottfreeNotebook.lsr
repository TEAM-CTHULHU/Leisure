# override these functions to redirect output to the HTML page
# where must be one of the following: 'DEBUG' 'INV' 'SCORE' 'LOOK' 'MSGS' 'HINTV' 'HINTN'
output where msg = js["$('.' + " (jsonStringify where id id) ".toLowerCase()).each(function(i, v) { v.innerHTML += " (jsonStringify msg id id) " + '<br>'; });"]
outputNoNl where msg = js["$('.' + " (jsonStringify where id id) ".toLowerCase()).each(function(i, v) { v.innerHTML += " (jsonStringify msg id id) "; });"]
clearScreen = js["$('.look').each(function(i, v) { v.innerHTML = ''; });"]
